{
  "include": ["src/**/*", "src/*.d.ts"],
  "compilerOptions": {
    "lib": ["DOM", "ES5"],
    "target": "ES5",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noUnusedLocals": true,
    "sourceMap": false,
    "pretty": true,
    "removeComments": false,
    "module": "CommonJS",
    "outDir": "dist",
    "baseUrl": "src"
  },
  "compileOnSave": true
}
